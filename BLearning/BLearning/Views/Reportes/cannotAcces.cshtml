@using BLearning.Models.Negocio;
@{
    PersonaEntidad objPersona = Session["personaLogin"] as PersonaEntidad;
    if (objPersona.Nivel.ToString() == "Docente")
    {
        Layout = "~/Views/Shared/_LayoutDocente.cshtml";
    }
    if (objPersona.Nivel.ToString() == "Administradores")
    {
        Layout = "~/Views/Shared/_LayoutAdministrador.cshtml";
    }
    if (objPersona.Nivel.ToString() == "Alumno")
    {
        Layout = "~/Views/Shared/_Layout.cshtml";
    }
    
}




